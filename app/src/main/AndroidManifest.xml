<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.elna.grandpaj">

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:icon="@mipmap/ic_launcher"
        android:theme="@style/AppTheme.NoActionBar"
        android:supportsRtl="true"
        android:name="com.elna.grandpaj.App">
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="AIzaSyCwe2b" />
        <!--
             The API key for Google Maps-based APIs.
        -->
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyCwe2bILbJSKuKPdELgLfDUeZn2plzpNP0" />
        <activity
            android:name="com.elna.grandpaj.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.elna.grandpaj.ChapterActivity"
            android:configChanges="orientation|screenSize"
            android:label="@string/app_name" />

        <activity
            android:name="com.elna.grandpaj.BioPictureActivity"
            android:configChanges="orientation|screenSize"
            android:label="@string/app_name" />

        <activity
            android:name="com.elna.grandpaj.timeline.TimeLineActivity"
            android:configChanges="orientation|screenSize"
            android:label="@string/app_name" />

    </application>

</manifest>
